<p id="notice"><%= notice %></p>

<h1>Listing Raws</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Filename</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @raws.each do |raw| %>
      <tr>
        <td><%= raw.user %></td>
        <td><%= raw.filename %></td>
        <td><%= link_to 'Show', raw %></td>
        <td><%= link_to 'Edit', edit_raw_path(raw) %></td>
        <td><%= link_to 'Destroy', raw, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Raw', new_raw_path %>
